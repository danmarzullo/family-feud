{
  "name": "family-feud",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bulma": "^0.7.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-keyboard-event-handler": "^1.3.1",
    "react-scripts": "2.0.4",
    "yaml": "^1.0.0"
  },
  "homepage": "./",
  "main": "src/main.js",
  "scripts": {
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "react-start": "BROWSER=none NODE_ENV=development $(npm bin)/react-scripts start",
    "electron-start": "ELECTRON_DEV=1 $(npm bin)/electron .",
    "start": "$(npm bin)/concurrently 'npm run react-start' '$(npm bin)/wait-on http://localhost:3000/ && npm run electron-start'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.0.1",
    "electron": "^3.0.2",
    "foreman": "^3.0.1",
    "wait-on": "^3.1.0"
  }
}
